{
	"name": "templater",
	"displayName": "Templater",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.36.0"
	},
	"categories": [
		"Other"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/RafaFP/templater.git"
	},
	"license": "SEE LICENSE IN README.md",
	"activationEvents": [
		"onCommand:searchPage.start",
		"onCommand:saveSnippet.edit"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "searchPage.start",
				"title": "templater > Search Code"
			},
			{
				"command": "saveSnippet.edit",
				"title": "templater > Save Snippet"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"dependencies": {
		"@types/copy-paste": "^1.1.30",
		"copy-paste": "^1.3.0",
		"stackexchange": "^1.2.0",
		"striptags": "^3.1.1",
		"appdata-path": "^1.0.0"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.36.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.0.0-next.0"
	}
}
